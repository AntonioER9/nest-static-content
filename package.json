{
  "name": "static-content",
  "version": "1.0.0",
  "description": "Static Content API",
  "author": "cda",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=16.16.0",
    "npm": ">=8.11.0",
    "yarn": "please-use-npm"
  },
  "scripts": {
    "compile": "tsc -p tsconfig.build.json",
    "prebuild": "rimraf dist",
    "build": "nest build -p tsconfig.json",
    "format": "prettier -c '**/*.ts'",
    "format:fix": "prettier -w '**/*.ts'",
    "lint": "eslint . --ext .ts --max-warnings 3",
    "spell": "cspell --config ./cspell.json --no-progress --show-suggestions --show-context '**/*.ts' '**/*.md'",
    "audit": "npm audit --audit-level=critical",
    "validate": "npm run format && npm run lint && npm run compile",
    "docker:start": "docker compose up -d",
    "docker:build": "docker compose build",
    "docker:restart": "docker compose restart",
    "docker:stop": "docker compose stop",
    "docker:down": "docker compose down",
    "docker:down:complete": "docker compose down --remove-orphans -v",
    "db:debug": "mikro-orm debug",
    "db:create": "mikro-orm schema:fresh --run",
    "db:update": "mikro-orm schema:update --run",
    "db:drop": "mikro-orm schema:drop --run",
    "db:seed": "mikro-orm seeder:run",
    "db:ci": "mikro-orm schema:fresh --seed --run",
    "start": "nest start -p tsconfig.json",
    "start:dev": "nest start --watch -p tsconfig.build.json",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch -p tsconfig.build.json",
    "start:prod": "node dist/main",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --coverage --coverageDirectory=\"./coverage\" --ci --reporters=jest-junit --watchAll=false",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.json"
  },
  "dependencies": {
    "@cencosud-ds/cda-flags": "1.1.0",
    "@golevelup/ts-jest": "0.3.7",
    "@mikro-orm/cli": "5.6.3",
    "@mikro-orm/core": "5.6.3",
    "@mikro-orm/nestjs": "5.1.5",
    "@mikro-orm/postgresql": "5.6.3",
    "@mikro-orm/reflection": "5.6.3",
    "@mikro-orm/seeder": "5.6.3",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "9.0.11",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.11",
    "@nestjs/jwt": "10.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.11",
    "@nestjs/swagger": "6.1.4",
    "@nestjs/terminus": "9.1.1",
    "@types/newrelic": "7.0.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "helmet": "5.1.1",
    "json5": "2.2.2",
    "jsonwebtoken": "9.0.0",
    "newrelic": "9.0.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.11",
    "@tsconfig/node16": "1.0.3",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.7",
    "@types/node": "18.7.6",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-unicorn": "43.0.2",
    "git-validate": "2.2.4",
    "jest": "28.1.3",
    "jest-junit": "14.0.0",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "tsc-watch": "5.0.3",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "tsConfigPath": "./tsconfig.orm.json",
    "configPaths": [
      "./src/config/orm.config.ts",
      "./dist/config/orm.config.js"
    ]
  },
  "pre-commit": [
    "format",
    "lint",
    "compile"
  ],
  "pre-push": [
    "build",
    "test:cov",
    "test:e2e",
    "audit"
  ]
}
